%nav.navbar-inverse
  %ul.nav.navbar-nav.side-nav
    %li
      = link_to new_room_path do
        %span.glyphicon.glyphicon-plus
          Create room
    - if !@room_list.nil?
      - @room_list.each do |room|
        - if room.id==@room_id.to_i
          %li.active
            =link_to "#{room.name}",room_path(:id=>room.id),:room_id => room.id
        -else
          %li
            =link_to "#{room.name}",room_path(:id=>room.id), :room_id => room.id